    "normalize-scss": "^7.0.1",
    "resolve-url-loader": "^3.1.4",
    "sass": "^1.44.0",
    "sass-loader": "^11.1.1",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "main": "webpack.mix.js",
  "repository": {
    "type": "git",
    "url": "git@gitlab.web.fh-kufstein.ac.at:gunharth/calculator.git"
